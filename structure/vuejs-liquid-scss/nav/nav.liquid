{% set data = data|default([
	{title:'Item 1', link:'#1'},
	{title:'Item 2', link:'#2'},
	{title:'Item 3', link:'#3'}
]) %}

<nav block="{{ name }}">

	<ul>
		{% for item in name %}
			<li element="item"><a href="{{ item.link }}" class="{{ item.classes|default }}" target="{{ item.target|default('_self') }}" rel="{% if item.target|default == '_blank' %}noreferrer noopener{% endif %}">{{ item.title }}</a></li>
		{% endfor %}
	</ul>
</nav>
